generator client {
  provider        = "prisma-client-js"
  output          = "./src/generated/prisma"
  binaryTargets   = ["native", "rhel-openssl-3.0.x"] // Tambahkan baris ini
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model playing_with_neon {
  id    Int     @id @default(autoincrement())
  name  String
  value Float?  @db.Real
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
}